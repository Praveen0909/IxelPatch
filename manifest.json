{
  "manifest_version": 2,

  "name": "E-Box | Ixel Patch",
  "description": "Allows you to copy and paste over those restriced webpage",
  "version": "1.0",
    
    
    "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": [ "inject.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
